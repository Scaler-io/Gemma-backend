{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LoggerConfigOption": {
    "OverrideSource": "Microsoft",
    "OutputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] {NewLine} {Properties} {NewLine} {NewLine} {Message} {Exception}"
  },
  "CacheSettings": {
    "ConnectionString": "localhost:6379"
  },
  "GrpcClient": {
    "DiscountUrl": "http://localhost:5002"
  },
  "RabbitmqSettings": {
    "ConnectionString": "amqp://user:Passw0rd@localhost:5672"
  }
}
